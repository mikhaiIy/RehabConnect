@* ************** Content ************** *@

<div class="d-flex col-lg-10 align-items-center justify-content-center p-sm-5 p-3">
  <div class="w-px-1100">
    <div class="bs-stepper wizard-numbered shadow-none">
      <div class="bs-stepper-header border-bottom-0">
        <div class="step" data-target="#accountDetailsValidation">
          <button type="button" class="step-trigger">
            <span class="bs-stepper-circle">1</span>
            <span class="bs-stepper-label">
            <span class="bs-stepper-title">Report Details</span>
            <span class="bs-stepper-subtitle">Enter Report Basic Details</span>
            </span>
          </button>
        </div>
        <div class="line">
          <i class="ti ti-chevron-right"></i>
        </div>
        <div class="step" data-target="#personalInfoValidation">
          <button type="button" class="step-trigger">
            <span class="bs-stepper-circle">2</span>
            <span class="bs-stepper-label">
            <span class="bs-stepper-title">Subjective Assessment</span>
            <span class="bs-stepper-subtitle">Report for S-Subjective Assessment</span>
            </span>
          </button>
        </div>
        <div class="line">
          <i class="ti ti-chevron-right"></i>
        </div>
        <div class="step" data-target="#billingLinksValidation">
          <button type="button" class="step-trigger">
            <span class="bs-stepper-circle">3</span>
            <span class="bs-stepper-label">
            <span class="bs-stepper-title">Objective Assessment</span>
            <span class="bs-stepper-subtitle">Report for O-Objective Assessment</span>
            </span>
          </button>
        </div>
      </div>
      <div class="bs-stepper-content">
        <form method="post" id="ParentDetailForm">
          <input asp-for="ReportID" hidden />
          <!-- Account Details -->
          <div id="accountDetailsValidation" class="content">
            <div class="content-header mb-4">
              <h3 class="mb-1">Report Details</h3>
              <p>Enter Student Report Details.</p>
            </div>
            <div class="row g-3">
              <div class="col-sm-12">
                <label class="form-label" asp-for="StudentID">Choose Student: </label>
                <select class="form-select" asp-for="StudentID" asp-items="ViewBag.Student"></select>
                <span asp-validation-for="StudentID" class="text-danger"></span>
              </div>
              <div class="col-sm-3">
                <label class="form-label" asp-for="NoSession">No. of Session Attended</label>
                <input asp-for="NoSession" type="number" class="form-control" placeholder="#" />
                <span asp-validation-for="NoSession" class="text-danger"></span>
              </div>
              <div class="col-sm-12">
                <label class="form-label" asp-for="DateReport">Date of the Report:  </label>
                <input asp-for="DateReport" type="datetime-local" class="form-control" />
                <span asp-validation-for="DateReport" class="text-danger"></span>
              </div>
              <label class="form-label">Treatment Provided:</label>
              <div class="col-sm-12 form-check">
                <input class="form-check-input" type="checkbox" asp-for="IndividualTherapy" />
                <label class="form-check-label" asp-for="IndividualTherapy">
                  Individual Therapy
                </label>
              </div>
              <div class="col-sm-12 form-check">
                <input class="form-check-input" type="checkbox" asp-for="GroupTherapy" />
                <label class="form-check-label" asp-for="GroupTherapy">
                  Group Therapy
                </label>
              </div>
              <div class="col-sm-12 form-check">
                <input class="form-check-input" type="checkbox" asp-for="EarlyIntervention" />
                <label class="form-check-label" asp-for="EarlyIntervention">
                  Early Intervention
                </label>
              </div>
              <div class="col-sm-12 form-check">
                <input class="form-check-input" type="checkbox" asp-for="BehaviorManagement" />
                <label class="form-check-label" asp-for="BehaviorManagement">
                  Behavior Management
                </label>
              </div>
              <div class="col-12 d-flex justify-content-between mt-4">
                <button type="button" class="btn btn-label-secondary btn-prev" disabled>
                  <i class="ti ti-arrow-left ti-xs me-sm-1 me-0"></i>
                  <span class="align-middle d-sm-inline-block d-none">Previous</span>
                </button>
                <button type="button" class="btn btn-primary btn-next">
                  <span class="align-middle d-sm-inline-block d-none me-sm-1 me-0">Next</span>
                  <i class="ti ti-arrow-right ti-xs"></i>
                </button>
              </div>
            </div>
          </div>
          <!-- Personal Info -->
          <div id="personalInfoValidation" class="content">
            <div class="content-header mb-4">
              <h3 class="mb-1">Subjective Assessment</h3>
              <p>Check the box if Yes for the S-Subjective Assessment</p>
            </div>
            <div class="row g-3">
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="CanEnterSelf" />
                <label class="form-check-label" asp-for="CanEnterSelf">
                  Can Enter by Self
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="WithPrompting" />
                <label class="form-check-label" asp-for="WithPrompting">
                  With Prompting
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="DifficultiesSeparateWithParents" />
                <label class="form-check-label" asp-for="DifficultiesSeparateWithParents">
                  Difficulties Separate With Parents
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="WithCryingAndRefuse" />
                <label class="form-check-label" asp-for="WithCryingAndRefuse">
                  With Crying And Refuse
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="GreetingWithPrompt" />
                <label class="form-check-label" asp-for="GreetingWithPrompt">
                  Greeting With Prompt
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="GreetingBySelf" />
                <label class="form-check-label" asp-for="GreetingBySelf">
                  Greeting By Self
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="Mute" />
                <label class="form-check-label" asp-for="Mute">
                  Mute
                </label>
              </div>
              <div class="col-sm-6 form-check">
                <input class="form-check-input" type="checkbox" asp-for="RefuseToEnter" />
                <label class="form-check-label" asp-for="RefuseToEnter">
                  Refuse To Enter
                </label>
              </div>
              <div>
                <label for="SubjectiveAssessmentNotes" class="form-label" asp-for="SubjectiveAssessmentNotes">Subjective Assessment Progress Note</label>
                <textarea class="form-control" asp-for="SubjectiveAssessmentNotes" rows="3"></textarea>
                <span asp-validation-for="SubjectiveAssessmentNotes" class="text-danger"></span>
              </div>
              <div class="col-12 d-flex justify-content-between mt-4">
                <button type="button" class="btn btn-label-secondary btn-prev" disabled>
                  <i class="ti ti-arrow-left ti-xs me-sm-1 me-0"></i>
                  <span class="align-middle d-sm-inline-block d-none">Previous</span>
                </button>
                <button type="button" class="btn btn-primary btn-next">
                  <span class="align-middle d-sm-inline-block d-none me-sm-1 me-0">Next</span>
                  <i class="ti ti-arrow-right ti-xs"></i>
                </button>
              </div>
            </div>
          </div>
